{
  "name": "umd-bootstrapping-amd-amd",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "compile": "cross-env NODE_ENV=production webpack --progress --profile",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint src/",
    "server": "cross-env NODE_ENV=test webpack-dev-server",
    "test": "run-s compile test:prep test:run",
    "test:prep": "cp dist/*.js ../public/js/umd-bootstrapping-amd-amd/",
    "test:run": "cross-env NODE_ENV=test ava --verbose",
    "watch": "run-p watch:src watch:test",
    "watch:src": "chokidar 'src/**/*.js' -c 'run-s compile'",
    "watch:test": "chokidar 'test/**/*.js' -c 'run-s test:prep test:run'"
  },
  "author": "Kenichi Ohtomi",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-flow": "^6.23.0",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^3.2.4",
    "eslint": "^3.16.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "flow-bin": "^0.40.0",
    "flow-typed": "^2.0.0",
    "jsdom": "^9.12.0",
    "npm-run-all": "^4.0.2",
    "requirejs": "^2.3.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "src/**/*.{js,jsx}"
    ],
    "require": [
      "./test/setup.js"
    ],
    "babel": "inherit"
  }
}
